{
  "name": "fix Ficbook styles",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Adds slyle and js files to Ficbook pages, in case you cannot load them",
  "browser_specific_settings": {
    "gecko": { "id": "{4b30d7a9-612a-43d3-8055-940296fb1f5c}" }
  },
  "web_accessible_resources": [
    "app.e070967e3f978c7f0296.js",
    "775.6f6128e1437b5d15972e.js",
    "app.df4e75f7ad1d24d4125c.css",
    "logo.svg"
  ],
  "permissions": ["activeTab", "scripting", "tabs", "https://ficbook.net/*/"],
  "background": {
    "scripts": ["script.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["app.df4e75f7ad1d24d4125c.css"],
      "run_at": "document_start"
    }
  ]
}
